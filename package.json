{
  "name": "react-typescript-template",
  "version": "2.1.0",
  "description": "Boilerplate for react-typescript projects",
  "main": "index.tsx",
  "scripts": {
    "start": "run-s eslint:fix webpack:serve",
    "watch": "run-s eslint:fix webpack:watch",
    "build": "cross-env NODE_ENV=production run-s eslint:fix webpack:build",
    "webpack:serve": "webpack serve --config \"./scripts/webpack.dev.config.ts\" --progress",
    "webpack:watch": "webpack --config \"./scripts/webpack.dev.config.ts\" --progress --watch",
    "webpack:build": "webpack --config \"./scripts/webpack.prod.config.ts\" --progress",
    "analyze": "cross-env ANALYZE_BUILD=true webpack --config \"./scripts/webpack.prod.config.ts\" --progress",
    "eslint:fix": "eslint --config \"./.eslintrc.js\" --color --fix \"./src/**/*.{ts,tsx}\"",
    "config:test": "webpack configtest \"./scripts/webpack.dev.config.ts\" && webpack configtest \"./scripts/webpack.prod.config.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "preinstall": "node scripts/disallow-npm.js"
  },
  "keywords": [
    "react",
    "typescript",
    "template"
  ],
  "author": "Searates",
  "license": "ISC",
  "engines": {
    "node": ">=12.16.3",
    "npm": ">=6.14.4",
    "pnpm": ">=6"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@redux-devtools/extension": "^3.2.2",
    "@svgr/core": "^6.2.1",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/dom": "^8.14.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "27.5.2",
    "@types/moxios": "^0.4.15",
    "@types/node": "^17.0.36",
    "@types/postcss-flexbugs-fixes": "^4.2.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/react-redux": "^7.1.24",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "camelcase": "6.2.1",
    "compression-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-webpack-plugin": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "27.4.3",
    "jest-environment-jsdom": "27.5.1",
    "mini-css-extract-plugin": "^2.6.1",
    "msw": "^0.43.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "ts-jest": "27.1.4",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1"
  }
}
